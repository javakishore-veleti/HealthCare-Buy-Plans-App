{
  "name": "healthcare-buy-plans-app",
  "version": "1.0.0",
  "description": "Healthcare Insurance E-Commerce Platform - Your Health Plans",
  "author": "Workshop Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "------------------ ANGULAR UI ------------------": "",
    "ui:install": "cd front_end_code/healthcare_plans_ui && npm install",
    "ui:start": "cd front_end_code/healthcare_plans_ui && npm start",
    "ui:build": "cd front_end_code/healthcare_plans_ui && ng build",
    "ui:build:prod": "cd front_end_code/healthcare_plans_ui && ng build --configuration=production",
    "ui:test": "cd front_end_code/healthcare_plans_ui && ng test",
    "ui:lint": "cd front_end_code/healthcare_plans_ui && ng lint",
    "------------------ DJANGO BACKEND ------------------": "",
    "api:install": "cd back_office && . venv/bin/activate && pip install -r requirements.txt",
    "api:migrate": "cd back_office && . venv/bin/activate && python manage.py migrate",
    "api:makemigrations": "cd back_office && . venv/bin/activate && python manage.py makemigrations",
    "api:start": "cd back_office && . venv/bin/activate && python manage.py runserver 8000",
    "api:createsuperuser": "cd back_office && . venv/bin/activate && DJANGO_SUPERUSER_PASSWORD=admin python manage.py createsuperuser --noinput --email admin@admin.com --mobile 1234567890 || echo 'Superuser already exists'",
    "api:shell": "cd back_office && . venv/bin/activate && python manage.py shell",
    "------------------ DOCKER SERVICES ------------------": "",
    "docker:mysql:start": "cd dev_ops/local_development/mysql && docker-compose up -d",
    "docker:mysql:stop": "cd dev_ops/local_development/mysql && docker-compose down",
    "docker:postgres:start": "cd dev_ops/local_development/postgres && docker-compose up -d",
    "docker:postgres:stop": "cd dev_ops/local_development/postgres && docker-compose down",
    "docker:wiremock:start": "cd dev_ops/local_development/wiremock && docker-compose up -d",
    "docker:wiremock:stop": "cd dev_ops/local_development/wiremock && docker-compose down",
    "docker:status": "docker ps --format 'table {{.Names}}\t{{.Status}}\t{{.Ports}}'",
    "------------------ LIQUIBASE ------------------": "",
    "db:migrate": "cd back_office/db/liquibase && liquibase update",
    "db:rollback": "cd back_office/db/liquibase && liquibase rollback-count 1",
    "db:status": "cd back_office/db/liquibase && liquibase status",
    "------------------ COMBINED COMMANDS ------------------": "",
    "setup": "npm run ui:install",
    "setup:all": "npm run ui:install && npm run api:install",
    "start": "npm run ui:start",
    "start:api": "npm run api:start",
    "dev:ui": "npm run ui:start",
    "dev:api": "npm run api:start"
  },
  "keywords": [
    "healthcare",
    "insurance",
    "angular",
    "django",
    "gcp",
    "workshop"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/HealthCare-Buy-Plans-App.git"
  },
  "dependencies": {
    "zone.js": "^0.16.0"
  }
}